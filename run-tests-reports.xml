<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="suite-packages" preserve-order="false">

    <parameter name="browser" value="firefox"></parameter>

    <test name="test1">
        <parameter name="browser" value="chrome"></parameter>
        <classes>
            <class name="com.pragmatic.testng.group4.TestNGExample5"/>
        </classes>
    </test>


    <test name="test2" enabled="true">
        <classes>
            <class name="com.pragmatic.testng.group4.TestNGExample4"/>
        </classes>
    </test>

    <test name="test3" enabled="false">
        <classes>
            <class name="com.pragmatic.testng.group4.TestNGExample3"/>
            <class name="com.pragmatic.testng.group4.TestNGExample4"/>
        </classes>
    </test>

    <listeners>
        <listener class-name="com.pragmatic.testng.PTLTestSuiteListener"></listener>
    </listeners>


</suite>